{
  "name": "@relayplane/proxy",
  "version": "0.1.10",
  "description": "RelayPlane Proxy - Intelligent AI model routing for cost optimization. 100% local, zero cloud dependency.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "relayplane-proxy": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "relayplane",
    "proxy",
    "ai",
    "llm",
    "routing",
    "cost-optimization",
    "anthropic",
    "openai",
    "gemini",
    "grok",
    "moonshot",
    "local-first",
    "openclaw",
    "cursor",
    "aider"
  ],
  "author": "RelayPlane",
  "license": "MIT",
  "homepage": "https://relayplane.com/integrations/openclaw",
  "repository": {
    "type": "git",
    "url": "https://github.com/RelayPlane/proxy.git"
  },
  "bugs": "https://github.com/RelayPlane/proxy/issues",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
